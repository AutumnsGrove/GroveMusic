{
  "name": "grovemusic",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "sst dev",
    "build": "sst build",
    "deploy": "sst deploy",
    "deploy:prod": "sst deploy --stage production",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "db:migrate": "wrangler d1 execute grovemusic-db --file=./schema.sql",
    "db:migrate:local": "wrangler d1 execute grovemusic-db --file=./schema.sql --local"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241205.0",
    "@sveltejs/adapter-cloudflare": "^5.0.0",
    "@sveltejs/kit": "^2.15.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "sst": "^3.0.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^6.0.0",
    "wrangler": "^3.99.0"
  },
  "dependencies": {
    "sst": "^3.0.0"
  }
}
